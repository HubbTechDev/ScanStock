    "expo": "~53.0.0",
    "expo-camera": "~16.0.8",
    "expo-constants": "~17.0.0",
    "expo-linking": "~7.0.0",
    "expo-router": "~4.0.0",
    "expo-status-bar": "~2.0.0",
    "lucide-react-native": "^0.469.0",
    "nativewind": "^4.1.23",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.7",
    "react-native-gesture-handler": "~2.21.2",
    "react-native-reanimated": "~3.16.6",
    "react-native-safe-area-context": "5.0.0",
    "react-native-screens": "~4.4.0",
